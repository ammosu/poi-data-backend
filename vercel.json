{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "*.py": {
      "excludeFiles": [
        "tests/**",
        "docs/**",
        "*.log",
        "node_modules/**",
        "*.md"
      ]
    }
  }
}
